<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.veryworks.iyeongjun.seoulssul.MainActivity">

    <com.lorentzos.flingswipe.SwipeFlingAdapterView
        android:id = "@+id/frame"
        android:layout_width="wrap_content"
        android:layout_height="505dp"
        android:text="Hello World!"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/btnAR"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:text="Button" />

    <Button
        android:id="@+id/btnWrite"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Button"
        android:layout_marginStart="13dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/btnMypage" />

    <Button
        android:id="@+id/btnRedirect"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Button"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/btnAR" />

    <Button
        android:id="@+id/btnMypage"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Button"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/btnRefresh"
        android:layout_marginStart="11dp" />

    <Button
        android:id="@+id/btnRefresh"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Button"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <Button
            android:id="@+id/button3"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:text="Button" />
    </RelativeLayout>

</RelativeLayout>
